{
    "manifest_version": 3,
    "name": "Salah Signal",
    "description": "Never miss a prayer again...",
    "version": "1.0",
    "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }, 
    "author": "Naeem, Min, Ramah, Melanie",
    "action": {
      "default_popup":"index.html",
      "default_title":"Salah Signal - Never miss a prayer again..."
    },
    "permissions": [
      "geolocation",
      "notifications"
    ],
    "background": {
      "service_worker":"service-worker.js"
    },
    "default_locale": "en"
    }
  